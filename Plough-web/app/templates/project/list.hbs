<div class="row">
  <div>
    <p class="page-header pmo-header title-p">项目管理-项目列表</p>
  </div>
</div>
<div class="row">
  <div class="col-md-4 col-lg-4 no-padding-left">
    <div class="form-group input-group">
      {{input value=searchWordInput placeholder='请按项目名称，项目编号/商机编号或者项目经理等信息模糊搜索' key-up="searchedItemFromWord"  class="search-input form-control"}}
      <span class="input-group-btn">
      <button class="btn btn-default" type="button"><i class="glyphicon glyphicon-search"></i>
      </button>
  </span>
    </div>
  </div>


  <div class="col-md-2 col-lg-2">
    <div class="form-group input-group">
      {{#x-select value=editProject.purchaseType action="showDateFieldChanged"  as |xs|}}
        {{#xs.option value='all'}}显示全部字段{{/xs.option}}
        {{#xs.option value='important'}}项目重要信息{{/xs.option}}
        {{#xs.option value='basic'}}项目基本信息{{/xs.option}}
        {{#xs.option value='customer'}}客户侧信息{{/xs.option}}
        {{#xs.option value='suyan'}}苏研侧信息{{/xs.option}}
        {{#xs.option value='status'}}项目状态{{/xs.option}}
        {{#xs.option value='process'}}项目进展{{/xs.option}}
      {{/x-select}}
    </div>
  </div>

  <div class="col-md-2 col-lg-2">
    <div class="form-group input-group">
      {{#x-select value=risk action="riskChanged"  as |xs|}}
        {{#xs.option value=''}}当前风险度{{/xs.option}}
        {{#xs.option value='低'}}低{{/xs.option}}
        {{#xs.option value='中'}}中{{/xs.option}}
        {{#xs.option value='高'}}高{{/xs.option}}
      {{/x-select}}
    </div>
  </div>



  <div class="col-md-2 col-lg-2 img-height">
    {{#if session.isSuper}}
      <a class="icon-add glyphicon glyphicon-download-alt" href="index.php?m=Project&a=exportProjects"></a>

      <li title="导入excl" class="icon-add glyphicon glyphicon-upload" {{action 'showAddSomeProjectForm'}}></li>
    {{/if}}
    <li title="添加项目" class="icon-add glyphicon glyphicon-plus" {{action 'showAddProjectForm'}}></li>
  </div>
</div>




