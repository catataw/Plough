    {{!-- 固定列 --}} {{!-- left --}}
    <div class="fixed-table_fixed fixed-table_fixed-left">
        <div class="fixed-table_header-wraper">
            <table class="fixed-table_header" cellspacing="0" cellpadding="0" border="0">
                <thead>
                    <tr>
                        <th class="w-6">
                            <div class="table-cell">项目通称</div>
                        </th>
                    </tr>
                </thead>
            </table>
        </div>
        <div class="fixed-table_body-wraper {{if minDatas "min-data"}}">
            <table class="fixed-table_body" cellspacing="0" cellpadding="0" border="0">
                <tbody>
                    {{#each _Datas as |_treeValue|}}
                    <tr>
                        <td>
                            <div class="table-cell w-6" title={{_treeValue.projectAlias}}>{{_treeValue.projectAlias}}</div>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
    {{!-- right --}}
    <div class="fixed-table_fixed fixed-table_fixed-right">
        <div class="fixed-table_header-wraper">
            <table class="fixed-table_header" cellspacing="0" cellpadding="0" border="0">
                <thead>
                    <tr>
                        <th>
                            <div class="table-cell w-end">操作</div>
                        </th>
                    </tr>
                </thead>
            </table>
        </div>
        <div class="fixed-table_body-wraper {{if minDatas "min-data"}}">
            <table class="fixed-table_body" cellspacing="0" cellpadding="0" border="0">
                <tbody>
                    {{#each _Datas as |_treeValue|}}
                    <tr>
                        <td>
                            <div class="table-cell w-end">
                                <a class="my-list-btn">
                                    <span {{action 'editProjectForm' _treeValue}}>编辑</span>
                                </a>
                                <a class="my-list-btn">
                                    <span {{action 'history' _treeValue.id}}>历史</span>
                                </a>
                                {{!-- <a class="my-list-btn">
                                    <span {{action 'grade' _treeValue}}>评分</span>
                                </a> --}}
                            </div>
                        </td>
                    </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>
{{!-- footer --}}
<div style="float:right">
    共{{if filterLists filterData.length filterData.length}}条
</div>
